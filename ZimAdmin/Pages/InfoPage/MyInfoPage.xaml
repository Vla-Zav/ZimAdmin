<Page x:Class="ZimAdmin.Pages.MyInfoPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ZimAdmin.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="800"
      Title="MyInfoPage">

    <Grid>
        <StackPanel Margin="20">
            <TextBlock Text="Разработчик системы ZimAdmin" Style="{StaticResource tbTemplate}"/>
            <Image Source="/Resources/MyPhoto/MyPhoto.jpg" Height="auto" MaxHeight="500"/>
            <TextBlock Text="Завгородний Владислав" Style="{StaticResource tbTemplate}"/>
            <TextBlock Text="Сутдент группы 29-ИНС" Style="{StaticResource tbTemplate}"/>
        </StackPanel>
    </Grid>
</Page>
